{
  "name": "py-cage",
  "displayName": "pyCage",
  "publisher": "PranjalKitN",
  "icon": "assets/python.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/qKitNp/pyCage"
  },
  "description": "Fastest Python package manager for Visual Studio Code powered by uv by astral.sh",
  "version": "0.1.3",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "py-cage.addPackageLocal",
        "title": "pyCage: Add Python Packages to venv. (recommended)"
      },
      {
        "command": "py-cage.addPackageGlobal",
        "title": "pyCage: Add Python Packages to global environment. (legacy) "
      },
      {
        "command": "py-cage.debugInterpreter",
        "title": "pyCage: Debug Python Interpreter Setup"
      },
      {
        "command": "py-cage.makeRequirements",
        "title": "pyCage: Make a requirements.txt"
      },
      {
        "command": "py-cage.installFromRequirements",
        "title": "pyCage: Install from requirements.txt"
      },
      {
        "command": "py-cage.uvInit",
        "title": "pyCage: Initialize UV Project (uv init)"
      },
      {
        "command": "py-cage.uvAddPackage",
        "title": "pyCage: Add Package with UV (uv add)"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "fs": "^0.0.1-security",
    "vsce": "^2.15.0"
  }
}